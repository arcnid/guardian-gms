{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"module": "ESNext",
		"target": "ES2017",

		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/(authenticated)/device/_layout.js",
		"app/(authenticated)/dashboard/_layout.js"
	]
}
