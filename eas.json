{
	"cli": {
		"version": ">= 13.3.0",
		"appVersionSource": "remote"
	},
	"build": {
		"ios": {
			"distribution": "internal",
			"env": {
				"EXPO_USE_MODULAR_HEADERS": "1",
				"COCOAPODS_VERSION": "1.12.0"
			}
		},
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"EXPO_USE_MODULAR_HEADERS": "1",
				"COCOAPODS_VERSION": "1.12.0"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"EXPO_USE_MODULAR_HEADERS": "1",
				"COCOAPODS_VERSION": "1.12.0"
			}
		},
		"production": {
			"autoIncrement": true,
			"env": {
				"EXPO_USE_MODULAR_HEADERS": "1",
				"COCOAPODS_VERSION": "1.12.0"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
